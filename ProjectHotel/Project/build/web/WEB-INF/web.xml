<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>FilterTransfer</filter-name>
        <filter-class>sample.servlet.FilterTransfer</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FilterTransfer</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>sample.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterDateServlet</servlet-name>
        <servlet-class>sample.servlet.FilterDateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchRoomServlet</servlet-name>
        <servlet-class>sample.servlet.SearchRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAllRoomServlet</servlet-name>
        <servlet-class>sample.servlet.ShowAllRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateByManagerServlet</servlet-name>
        <servlet-class>sample.servlet.UpdateByManagerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportRoomServlet</servlet-name>
        <servlet-class>sample.servlet.ReportRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>sample.servlet.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageShowServlet</servlet-name>
        <servlet-class>sample.servlet.ManageShowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterDateServlet</servlet-name>
        <url-pattern>/FilterDateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRoomServlet</servlet-name>
        <url-pattern>/SearchRoomServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowAllRoomServlet</servlet-name>
        <url-pattern>/ShowAllRoomServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateByManagerServlet</servlet-name>
        <url-pattern>/UpdateByManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportRoomServlet</servlet-name>
        <url-pattern>/ReportRoomServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageShowServlet</servlet-name>
        <url-pattern>/ManageShowServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file/>
    </welcome-file-list>
</web-app>
